    <!-- http://purl.org/spar/fabio/Expression -->


    <owl:Class rdf:about="http://purl.org/spar/fabio/Expression">
        <rdfs:subClassOf rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#part"/>
                        <owl:allValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#partOf"/>
                        <owl:allValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#embodiment"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/spar/fabio/Manifestation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#realizationOf"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/spar/fabio/Work"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description xml:lang="en">A fabio:Expression can only have part or be part of another fabio:Expression.  Moreover, it can be a representation only of a fabio:Work, and  it can be embodied only in fabio:Manifestation(s).</dc:description>
        <rdfs:comment xml:lang="en">A subclass of FRBR expression, restricted to expressions of fabio:Works.  For your latest research paper, the preprint submitted to the publisher, and the final published version to which the publisher assigned a unique digital object identifier, are both expressions of the same work.  </rdfs:comment>
        <rdfs:label xml:lang="en">expression</rdfs:label>
    </owl:Class>
    


